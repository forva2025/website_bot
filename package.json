{
    "name": "deepseek-chatbot-backend",
    "version": "1.0.0",
    "description": "Backend server for Deepseek chatbot with enhanced knowledge base support",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "test": "node test-api.js",
      "build": "echo 'No build step required'",
      "deploy": "echo 'Deploy to your preferred platform'",
      "health": "curl http://localhost:3000/api/health",
      "logs": "tail -f server.log",
      "index": "node -e \"require('./knowledge_base_manager.js').prototype.updateIndex.call(require('./knowledge_base_manager.js'))\"",
      "stats": "node -e \"console.log(JSON.stringify(require('./knowledge_base_manager.js').prototype.getStats.call(require('./knowledge_base_manager.js')), null, 2))\""
    },
    "dependencies": {
      "express": "^4.18.2",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "axios": "^1.6.0",
      "uuid": "^9.0.1",
      "express-rate-limit": "^7.1.5",
      "cheerio": "^1.0.0-rc.12",
      "pdf-parse": "^1.1.1",
      "natural": "^6.10.4"
    },
    "devDependencies": {
      "nodemon": "^3.0.1"
    },
    "keywords": [
      "chatbot",
      "deepseek",
      "ai",
      "customer-service",
      "express",
      "api",
      "knowledge-base",
      "pdf-parsing",
      "web-scraping",
      "nlp"
    ],
    "author": "Your Name",
    "license": "MIT",
    "engines": {
      "node": ">=14.0.0"
    }
  }